class Solution {
public:
    bool isPalindrome(string s) {
        cin>>s;
        int sz = s.size();
        int start = s[i];
        int end = s[sz-1];
        int flag =false;
        for(int i=1;i<=sz;i++){
            if(s[i]==s[sz-i]) flag =true;
        }
        if(flag =true) cout<<"true";
        else cout<<"false";
        
        return 0;      
        
    }
};
